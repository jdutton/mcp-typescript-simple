{
  "version": 2,
  "framework": null,
  "routes": [
    {
      "src": "/health",
      "dest": "/api/health"
    },
    {
      "src": "/mcp",
      "dest": "/api/mcp"
    },
    {
      "src": "/auth/(.*)",
      "dest": "/api/auth"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/api/admin"
    }
  ],
  "functions": {
    "api/mcp.ts": {
      "maxDuration": 30
    },
    "api/auth.ts": {
      "maxDuration": 15
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "outputDirectory": "build"
}