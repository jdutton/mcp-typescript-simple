{
  "name": "@mcp-typescript-simple/adapter-vercel",
  "version": "0.9.1-rc.2",
  "description": "Vercel serverless adapter for MCP TypeScript Simple server",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./mcp": {
      "types": "./dist/mcp.d.ts",
      "import": "./dist/mcp.js"
    },
    "./auth": {
      "types": "./dist/auth.d.ts",
      "import": "./dist/auth.js"
    },
    "./health": {
      "types": "./dist/health.d.ts",
      "import": "./dist/health.js"
    },
    "./admin": {
      "types": "./dist/admin.d.ts",
      "import": "./dist/admin.js"
    },
    "./register": {
      "types": "./dist/register.d.ts",
      "import": "./dist/register.js"
    },
    "./well-known": {
      "types": "./dist/well-known.d.ts",
      "import": "./dist/well-known.js"
    },
    "./docs": {
      "types": "./dist/docs.d.ts",
      "import": "./dist/docs.js"
    }
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/ test/"
  },
  "dependencies": {
    "@mcp-typescript-simple/auth": "*",
    "@mcp-typescript-simple/config": "*",
    "@mcp-typescript-simple/example-tools-basic": "*",
    "@mcp-typescript-simple/example-tools-llm": "*",
    "@mcp-typescript-simple/http-server": "*",
    "@mcp-typescript-simple/observability": "*",
    "@mcp-typescript-simple/server": "*",
    "@mcp-typescript-simple/tools": "*",
    "@mcp-typescript-simple/tools-llm": "*",
    "@modelcontextprotocol/sdk": "^1.18.0",
    "@vercel/node": "^4.0.0",
    "vercel": "41.0.2",
    "yaml": "^2.6.1"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "eslint": "^9.0.0",
    "typescript": "^5.6.0",
    "vitest": "^4.0.6"
  },
  "keywords": [
    "mcp",
    "vercel",
    "serverless",
    "adapter"
  ],
  "author": "Jeff Dutton",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdutton/mcp-typescript-simple.git",
    "directory": "packages/adapter-vercel"
  }
}
